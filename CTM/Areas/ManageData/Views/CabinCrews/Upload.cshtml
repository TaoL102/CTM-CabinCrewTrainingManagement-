
@{
    ViewBag.Title = "Upload";

}

<h2>Upload</h2>

@using (Html.BeginForm("Upload", "CabinCrews", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>乘务员名单上传</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })




        <div class="form-group">
            <div class="col-md-10">
                <input type="file" name="upload" id="ExcelUrl" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" class="btn btn-raised btn-round btn-default btn-file">

            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Upload" class="btn btn-default" />
            </div>
        </div>
    </div>
}